/* To configure RUCE's version & some features */

#define RUCE_VERSION_MAJOR @RUCE_VERSION_MAJOR@
#define RUCE_VERSION_MINOR @RUCE_VERSION_MINOR@
#define RUCE_VERSION_REVISION @RUCE_VERSION_REVISION@
#define RUCE_VERSION_PATCH @RUCE_VERSION_PATCH@
#define RUCE_VERSION_TAG "@RUCE_VERSION_TAG@"

#define RUCE_VERSION_STRING \
    "@RUCE_VERSION_MAJOR@.@RUCE_VERSION_MINOR@.@RUCE_VERSION_REVISION@ " \
    "- @RUCE_VERSION_TAG@ @RUCE_VERSION_PATCH@"

